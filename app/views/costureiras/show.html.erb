<%- model_class = Costureira -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:data) %>:</strong></dt>
  <dd><%= @costureira.data %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
  <dd><%= @costureira.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:valor_pago) %>:</strong></dt>
  <dd><%= @costureira.valor_pago %></dd>
  <dt><strong><%= model_class.human_attribute_name(:q_produto) %>:</strong></dt>
  <dd><%= @costureira.q_produto %></dd>
  <dt><strong><%= model_class.human_attribute_name(:obs) %>:</strong></dt>
  <dd><%= @costureira.obs %></dd>
  <dt><strong><%= model_class.human_attribute_name(:produto_id) %>:</strong></dt>
  <dd><%= @costureira.produto_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:quantidade_id) %>:</strong></dt>
  <dd><%= @costureira.quantidade_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              costureiras_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_costureira_path(@costureira), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              costureira_path(@costureira),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
